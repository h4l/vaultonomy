{
  "name": "reddit-vault-extension",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "_build": "npm run check && vite build",
    "__build": "npm run check && rollup -c --configPlugin @rollup/plugin-typescript --environment BUILD:production",
    "__dev": "rollup -c --configPlugin @rollup/plugin-typescript --environment BUILD:development",
    "__dev:watch": "rollup -c --configPlugin @rollup/plugin-typescript --environment BUILD:development --watch",
    "check": "npm run typecheck && npm run lint",
    "lint": "eslint src *.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@metamask/providers": "^14.0.2",
    "buffer": "^6.0.3",
    "json-rpc-2.0": "^1.4.2",
    "nanoevents": "^7.0.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "util": "^0.12.4",
    "wagmi": "^1.4.12",
    "webextension-polyfill": "^0.10.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.5",
    "@swc/core": "^1.3.101",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/chrome": "^0.0.254",
    "@types/deepmerge": "^2.2.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
